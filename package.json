{
	"name": "khanne-sh",
	"version": "1.3.1",
	"private": true,
	"engines": {
		"node": "20",
		"pnpm": "9"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"pregview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest --run",
		"prepare": "husky install",
		"gh-pages": "pnpm build && pnpx gh-pages -d build -t true"
	},
	"lint-staged": {
		"src/**/*.{svelte,ts}": [
			"eslint ."
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"devDependencies": {
		"@cspell/eslint-plugin": "^8.8.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/svelte": "^4.1.0",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-boundaries": "^4.2.1",
		"eslint-plugin-svelte": "^2.36.0",
		"gh-pages": "^5.0.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.2",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.2",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vite-node": "^1.6.0",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"clsx": "^2.1.1"
	},
	"packageManager": "pnpm@9.1.3+sha256.7f63001edc077f1cff96cacba901f350796287a2800dfa83fe898f94183e4f5f"
}
